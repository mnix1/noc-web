{
  "name": "noc-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "base-64": "^0.1.0",
    "lodash": "^4.17.5",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-relay": "^1.5.0",
    "react-scripts": "1.1.1",
    "relay-runtime": "^1.5.0"
  },
  "devDependencies": {
    "babel-plugin-relay": "^1.5.0",
    "eslint": "^4.19.1",
    "relay-compiler": "^1.5.0"
  },
  "scripts": {
    "lint": "eslint src",
    "clean": "rm -rf build && mkdir build",
    "get-schema": "node scripts/getSchema.js",
    "relay": "relay-compiler --src ./src --schema schema.graphql",
    "start": "node ./setup && react-scripts start",
    "build": "node ./setup && react-scripts build",
    "test": "node ./setup && react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "proxy": {
    "/": {
      "target": "http://localhost:8080",
      "pathRewrite": {
        "^/api": ""
      }
    }
  }
}
