{
  "name": "noc-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "base-64": "^0.1.0",
    "classnames": "^2.2.5",
    "first-person-controls": "^1.0.1",
    "improved-noise": "0.0.3",
    "lodash": "^4.17.5",
    "lost": "^8.2.1",
    "postcss": "^6.0.21",
    "postcss-cssnext": "^3.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-relay": "^1.5.0",
    "react-scripts": "1.1.4",
    "redux": "^3.7.2",
    "relay-runtime": "^1.5.0",
    "three": "^0.92.0"
  },
  "devDependencies": {
    "babel-plugin-relay": "^1.5.0",
    "eslint": "^4.19.1",
    "relay-compiler": "^1.5.0"
  },
  "scripts": {
    "lint": "eslint src",
    "clean": "rm -rf build && mkdir build",
    "get-schema": "node scripts/getSchema.js",
    "relay": "relay-compiler --src ./src --schema schema.graphql",
    "start": "node ./scripts/setup && react-scripts start",
    "build": "node ./scripts/setup && react-scripts build",
    "test": "node ./setup && react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "proxy": {
    "/": {
      "target": "http://localhost:8080",
      "pathRewrite": {
        "^/api": ""
      }
    }
  }
}
